	year = {2007},
	pages = {87--107},
	file = {Hawkins - 2007 - Processing typology and why psychologists need to .pdf:/home/user/Zotero/storage/K6CM67D6/Hawkins - 2007 - Processing typology and why psychologists need to .pdf:application/pdf}
}

