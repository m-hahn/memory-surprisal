	author = {Culbertson, Jennifer and Smolensky, Paul and Wilson, Colin},
	month = jul,
	year = {2013},
	keywords = {Learning biases, Artificial language learning, Bayesian modeling, Optimality theory, Typology, word order},
	pages = {392--424},
