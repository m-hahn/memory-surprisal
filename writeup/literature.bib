	pmcid = {PMC2859704},
	pages = {91--108},
	file = {PubMed Central Full Text PDF:/home/user/Zotero/storage/XEUNCV49/Kemmerer et al. - 2009 - Knowledge of the Semantic Constraints on Adjective.pdf:application/pdf}
